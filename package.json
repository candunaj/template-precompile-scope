{
  "name": "template-precompile-scope",
  "version": "0.0.0",
  "private": true,
  "repository": "",
  "license": "MIT",
  "author": "",
  "scripts": {
    "build": "pnpm --filter template-precompile-scope build",
    "lint": "pnpm --filter '*' lint",
    "lint:fix": "pnpm --filter '*' lint:fix",
    "prepare": "pnpm build",
    "start": "concurrently 'npm:start:*' --restart-after 5000 --prefix-colors cyan,white,yellow",
    "start:addon": "pnpm --filter template-precompile-scope start --no-watch.clearScreen",
    "start:test-app": "pnpm --filter test-app start",
    "test": "pnpm --filter '*' test"
  },
  "devDependencies": {
    "concurrently": "^7.2.1",
    "prettier": "^2.5.1"
  },
  "pnpm": {
    "overrides": {
      "@types/eslint": "^7.0.0",
      "babel-plugin-htmlbars-inline-precompile": "github:candunaj/babel-plugin-htmlbars-inline-precompile#precompile-template-scope-renamed-import-build"
    }
  }
}
